<launch>

            <group ns="r1">
                <arg name="ns" value="r1"/>
                <param name="robot_description" command="$(find xacro)/xacro $(find mascot)/urdf/model.xacro robot_name:=$(arg ns)" />                                        
                <include file="$(find mascot)/launch/one_robot.launch">
